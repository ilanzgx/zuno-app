{
  "name": "consolidador-investimentos",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -k \"npm run start:api\" \"npm run start:web\"",
    "start:web": "cd apps/web && npm run dev",
    "start:api": "cd apps/api && mvnw spring-boot:run",
    "start:api:debug": "cd apps/api && mvnw spring-boot:run -Dspring-boot.run.jvmArguments='-agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=*:8000'",
    "maven:resolve": "cd apps/api && mvnw dependency:resolve",
    "maven:build": "cd apps/api && mvnw clean install -DskipTests",
    "maven:list": "cd apps/api && mvnw dependency:tree"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}
